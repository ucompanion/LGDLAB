{
  // live sass
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/assets/css/"
    },
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/assets/css/"
    }
  ],
  "liveSassCompile.settings.generateMap": false,
  "liveServer.settings.port": 5000,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "javascript.preferences.quoteStyle": "double",
  
}
